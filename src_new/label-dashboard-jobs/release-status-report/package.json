{
  "name": "release-status-report-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda function to generate release and song status Excel report and email to superadmin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test:local": "npm run build && node dist/local-test.js",
    "package": "npm run build && bestzip release-status-report.zip dist node_modules package.json",
    "deploy": "aws lambda update-function-code --function-name release-status-report --zip-file fileb://release-status-report.zip"
  },
  "keywords": [
    "aws",
    "lambda",
    "release",
    "song",
    "status",
    "report"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "exceljs": "^4.4.0",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.140",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.15",
    "bestzip": "^2.2.1",
    "dotenv": "^16.4.5",
    "typescript": "^5.5.3"
  }
}
